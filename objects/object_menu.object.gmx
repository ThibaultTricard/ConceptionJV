        // New game
        case 0:
        {
            room_goto_next();
            break;
        }
         
        // Scores       
        case 1:
        {
            room_goto(scoresRoom);
            break;
        }
           
        // Quit 
        case 2:
        {
            game_end();
            break;
        }
        
        default: break;
    }
}
